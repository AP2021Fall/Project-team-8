<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="body" alignment="TOP_CENTER" prefHeight="189.0" prefWidth="250.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="header" alignment="CENTER" contentDisplay="CENTER" prefHeight="59.0" prefWidth="250.0" text="Invite To Board" />
    <Label fx:id="separator" minHeight="1.0" prefHeight="1.0" prefWidth="250.0">
      <VBox.margin>
         <Insets bottom="20.0" />
      </VBox.margin></Label>
    <TextField fx:id="user" prefHeight="48.0" prefWidth="250.0" promptText="Email address of username" />
   <Label prefHeight="44.0" prefWidth="28.0" />
    <Button fx:id="inviteBtn" mnemonicParsing="false" prefHeight="25.0" prefWidth="250.0" text="Invite" />
</fx:root>
